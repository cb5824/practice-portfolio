<p>Projects page...</p>

<ul>
    <% @projects.each do |project| %>
      <li>
        <a href=<%= project.url %> class="project_link">
        <div class="project_listing">
            <h2><%= project.title %></h2>
            <p><%= project.description %></p>
        </div>
      </a>
      </li>
    <% end %>
</ul>
